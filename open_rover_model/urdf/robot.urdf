<?xml version="1.0"?>
<robot name="my_robot">

  <!-- Constants -->
  <property name="M_PI" value="3.1415926535897931" />
  <property name="M_PI_2" value="1.5707963267948966" />
  <property name="M_PI_4" value="0.7853981633974483" />

<?xml version="1.0"?>
<robot name="open_rover" xmlns:xacro="http://ros.org/wiki/xacro">
  <!-- Dummy Root Link -->
  <link name="dummy_root">
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
      <material name="transparent"><color rgba="0 0 0 0"/></material>
    </visual>
    <collision>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry><sphere radius="0.01"/></geometry>
    </collision>
  </link>

  <!-- Materials -->
  <material name="black"><color rgba="0 0 0 1"/></material>
  <material name="blue"><color rgba="0 0 0.8 1"/></material>
  <material name="grey"><color rgba="0.2 0.2 0.2 1"/></material>
  <material name="red"><color rgba="0.8 0 0 1"/></material>

  <!-- Main Frame Links -->
  <link name="main_frame_side1">
    <visual><geometry><box size="0.76 0.04 0.04"/></geometry><material name="blue"/></visual>
    <collision><geometry><box size="0.76 0.04 0.04"/></geometry></collision>
    <inertial><mass value="1.8"/><inertia ixx="0.002" ixy="0" ixz="0" iyy="0.087" iyz="0" izz="0.087"/></inertial>
  </link>
  <link name="main_frame_side2">
    <visual><geometry><box size="0.76 0.04 0.04"/></geometry><material name="blue"/></visual>
    <collision><geometry><box size="0.76 0.04 0.04"/></geometry></collision>
    <inertial><mass value="1.8"/><inertia ixx="0.002" ixy="0" ixz="0" iyy="0.087" iyz="0" izz="0.087"/></inertial>
  </link>
  <link name="main_frame_end1">
    <visual><geometry><box size="0.75 0.04 0.04"/></geometry><material name="blue"/></visual>
    <collision><geometry><box size="0.75 0.04 0.04"/></geometry></collision>
    <inertial><mass value="1.8"/><inertia ixx="0.002" ixy="0" ixz="0" iyy="0.085" iyz="0" izz="0.085"/></inertial>
  </link>
  <link name="main_frame_end2">
    <visual><geometry><box size="0.75 0.04 0.04"/></geometry><material name="blue"/></visual>
    <collision><geometry><box size="0.75 0.04 0.04"/></geometry></collision>
    <inertial><mass value="1.8"/><inertia ixx="0.002" ixy="0" ixz="0" iyy="0.085" iyz="0" izz="0.085"/></inertial>
  </link>
  <link name="main_frame_leg1">
    <visual><geometry><box size="0.04 0.04 0.5"/></geometry><material name="blue"/></visual>
    <collision><geometry><box size="0.04 0.04 0.5"/></geometry></collision>
    <inertial><mass value="1.2"/><inertia ixx="0.025" ixy="0" ixz="0" iyy="0.025" iyz="0" izz="0.002"/></inertial>
  </link>
  <link name="main_frame_leg2">
    <visual><geometry><box size="0.04 0.04 0.5"/></geometry><material name="blue"/></visual>
    <collision><geometry><box size="0.04 0.04 0.5"/></geometry></collision>
    <inertial><mass value="1.2"/><inertia ixx="0.025" ixy="0" ixz="0" iyy="0.025" iyz="0" izz="0.002"/></inertial>
  </link>
  <link name="sheet_metal_box">
    <visual><geometry><box size="0.76 0.75 0.1"/></geometry><material name="grey"/></visual>
    <collision><geometry><box size="0.76 0.75 0.1"/></geometry></collision>
    <inertial><mass value="11.3"/><inertia ixx="0.54" ixy="0" ixz="0" iyy="0.55" iyz="0" izz="1.08"/></inertial>
  </link>

  <!-- Main Frame Joints -->
  <joint name="main_frame_side1_joint" type="fixed">
    <parent link="dummy_root"/>
    <child link="main_frame_side1"/>
    <origin xyz="0 0.375 0" rpy="0 0 0"/>
  </joint>
  <joint name="main_frame_side2_joint" type="fixed">
    <parent link="main_frame_side1"/>
    <child link="main_frame_side2"/>
    <origin xyz="0 -0.75 0" rpy="0 0 0"/>
  </joint>
  <joint name="main_frame_end1_joint" type="fixed">
    <parent link="main_frame_side1"/>
    <child link="main_frame_end1"/>
    <origin xyz="0.38 -0.375 0" rpy="0 0 1.5708"/>
  </joint>
  <joint name="main_frame_end2_joint" type="fixed">
    <parent link="main_frame_side2"/>
    <child link="main_frame_end2"/>
    <origin xyz="0.38 0.375 0" rpy="0 0 1.5708"/>
  </joint>
  <joint name="main_frame_leg1_joint" type="fixed">
    <parent link="main_frame_end1"/>
    <child link="main_frame_leg1"/>
    <origin xyz="0 0 -0.25" rpy="0 0 0"/>
  </joint>
  <joint name="main_frame_leg2_joint" type="fixed">
    <parent link="main_frame_end2"/>
    <child link="main_frame_leg2"/>
    <origin xyz="0 0 -0.25" rpy="0 0 0"/>
  </joint>
  <joint name="sheet_metal_box_joint" type="fixed">
    <parent link="main_frame_side1"/>
    <child link="sheet_metal_box"/>
    <origin xyz="0 -0.375 -0.05" rpy="0 0 0"/>
  </joint>
</robot>
